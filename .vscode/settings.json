{
  "gitlens.enabled": true,
  "gitlens.blame.enabled": true,
  "gitlens.blame.toggleMode": "file",
  "gitlens.codeLens.enabled": true,
  "gitlens.codeLens.recentChange.enabled": true,
  "gitlens.codeLens.authors.enabled": true,
  "gitlens.currentLine.enabled": true,
  "gitlens.hovers.enabled": true,
  "gitlens.hovers.currentLine.enabled": true,
  "gitlens.views.repositories.enabled": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.views.branches.enabled": true,
  "gitlens.views.remotes.enabled": true,
  "gitlens.views.stashes.enabled": true,
  "gitlens.views.tags.enabled": true,
  "gitlens.views.worktrees.enabled": true,
  "gitlens.views.commits.enabled": true,
  "gitlens.views.contributors.enabled": true,
  "gitlens.views.searchAndCompare.enabled": true,
  "gitlens.views.openCommitOnRemote": false,
  "gitlens.views.commitDetails.enabled": false,
  "gitlens.remotes": [],
  "gitlens.advanced.abbreviatedShaLength": 7,
  "gitlens.advanced.blame.customFormat": "${author|14} • ${date|short} • ${message}",
  "gitlens.advanced.fileHistoryShowAllBranches": false,
  "gitlens.gitStatusBar.enabled": true,
  "gitlens.statusBar.alignment": "left",
  "gitlens.statusBar.command": "gitlens.showQuickFileHistory",
  "gitlens.detectNestedRepositories": true,
  "gitlens.defaultDateFormat": "short",
  "gitlens.defaultDateStyle": "relative",
  "gitlens.defaultGravatarsStyle": "identicon",
  "gitlens.defaultTimeFormat": "short"
}
